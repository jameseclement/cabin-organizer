<h1>Create a Cabin!</h1>
<% if @cabin.errors.any? %>
  <div id="error_explanation">
    <h2>
      <%= pluralize(@cabin.errors.count, "error") %>
      prohibited this cabin from being saved:
    </h2>

    <ul>
    <% @cabin.errors.full_messages.each do |msg| %>
      <li><%= msg %></li>
    <% end %>
    </ul>
  </div>
<% end %>
<%= render 'form' %>
